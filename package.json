		"echarts": "^5.4.3",
		"echarts-for-react": "^3.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.11",
		"react-query": "^3.39.3"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.1.3",
